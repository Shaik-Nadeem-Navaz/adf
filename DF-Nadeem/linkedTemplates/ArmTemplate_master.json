			"type": "string",
			"defaultValue": "adminnadeem"
		},
		"Lnk_SqlServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-0BSKK4P"
		},
		"Lnk_SqlServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "datarise"
		},
		"Lnk_SqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DESKTOP-0BSKK4P\\HP"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
