		},
		"Lnk_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Lnk_AzureDataLakeStorage'"
		},
		"Lnk_AzureSqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Lnk_AzureSqlDatabase'"
		},
		"Lnk_SqlServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Lnk_SqlServer'"
		},
		"Lnk_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://nadeemadls.dfs.core.windows.net/"
		},
		"Lnk_AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
